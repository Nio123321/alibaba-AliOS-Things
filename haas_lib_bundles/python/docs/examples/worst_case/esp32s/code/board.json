{
    "name": "esp32",
    "version": "1.0.0",
    "io": {
      "uln2003_a": {
        "type": "GPIO",
        "port": 4,
        "dir": "output",
        "pull": "pushpull"
      },
      "uln2003_a_": {
        "type": "GPIO",
        "port": 0,
        "dir": "output",
        "pull": "pushpull"
      },
      "uln2003_b": {
        "type": "GPIO",
        "port": 2,
        "dir": "output",
        "pull": "pushpull"
      },
      "uln2003_b_": {
        "type": "GPIO",
        "port": 15,
        "dir": "output",
        "pull": "pushpull"
      },
      "magnetron": {
        "type": "GPIO",
        "port": 13,
        "dir": "input",
        "pull": "pullup"
      },
      "servo": {
        "type": "PWM",
        "port": 27
      },
      "photoresistor": {
        "type": "ADC",
        "port": 0,
        "atten": 2,
        "width": 3,
        "sampling": 12000000

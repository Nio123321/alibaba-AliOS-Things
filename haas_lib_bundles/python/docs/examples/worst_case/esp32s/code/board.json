      },
      "motorspeed": {
        "type": "GPIO",
        "port": 26,
        "dir": "irq",
        "pull": "pullup",
        "intMode": "falling"
      },
      "mfrc522_sda": {
        "type": "GPIO",
        "port": 5,
        "dir": "output",
        "pull": "pullup"
      },
      "mfrc522_rst":{
        "type":"GPIO",
        "port": 25,
        "dir": "output",
        "pull":"pullup"
      },
      "mfrc522": {
        "type": "SPI",
        "port": 3,

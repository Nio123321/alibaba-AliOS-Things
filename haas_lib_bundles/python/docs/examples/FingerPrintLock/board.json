    },
    "KEY_2": {
      "type": "GPIO",
      "port": 20,
      "dir": "irq",
      "pull": "pullup",
      "intMode": "rising"
    },
    "KEY_3": {
      "type": "GPIO",
      "port": 21,
      "dir": "irq",
      "pull": "pullup",
      "intMode": "rising"
    },
    "KEY_4": {
      "type": "GPIO",
      "port": 26,
      "dir": "irq",
      "pull": "pullup",
      "intMode": "rising"
    }
